{
  "name": "@fixdescriptorkit/ts-sdk",
  "version": "1.0.1",
  "description": "Transform FIX asset descriptors into canonical CBOR payloads and Merkle commitments for blockchain verification",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "FIXPARSER_LICENSE_KEY=FIXParser_ vitest run",
    "dev:test": "vitest",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "generate-merkle": "npx tsx scripts/generate-solidity-test-data.ts merkle > ../../contracts/src/generated/GeneratedMerkleData.sol",
    "generate-test-data": "npm run generate-merkle",
    "build:sbe-jar": "node scripts/build-sbe-jar.js",
    "test:encode": "tsx src/sbe/tests/index.ts",
    "test:decode": "tsx src/sbe/tests/index-decode.ts",
    "generate:sbe": "tsx scripts/generate-sbe-schema.ts"
  },
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./orchestraToSbe": {
      "import": "./dist/src/orchestraToSbe.js",
      "types": "./dist/src/orchestraToSbe.d.ts"
    },
    "./sbe": {
      "import": "./dist/src/sbe/lib.js",
      "types": "./dist/src/sbe/lib.d.ts"
    },
    "./sbe/encode": {
      "import": "./dist/src/sbe/encode.js",
      "types": "./dist/src/sbe/encode.d.ts"
    },
    "./sbe/decode": {
      "import": "./dist/src/sbe/decode.js",
      "types": "./dist/src/sbe/decode.d.ts"
    }
  },
  "files": [
    "dist/src",
    "lib",
    "lib/**",
    "src/sbe/agrona-ts",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "fix",
    "fix-protocol",
    "blockchain",
    "ethereum",
    "cbor",
    "merkle-tree",
    "financial",
    "asset-descriptor",
    "evm",
    "web3",
    "defi",
    "tradfi"
  ],
  "author": "Swapnil Raj <swp@nethermind.io>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swapnilraj/fixdescriptorkit-evm.git",
    "directory": "packages/fixdescriptorkit-typescript"
  },
  "homepage": "https://erc-fix.com",
  "bugs": {
    "url": "https://github.com/swapnilraj/fixdescriptorkit-evm/issues"
  },
  "dependencies": {
    "agrona": "file:./src/sbe/agrona-ts",
    "cbor-x": "^1.6.0",
    "@xmldom/xmldom": "^0.8.10",
    "fast-xml-parser": "^5.3.0",
    "fixparser": "^9.3.13",
    "viem": "^2.37.8",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
